{
    "compilerOptions": {
        "target": "es2016",
        "lib": ["ESNext"],
        "module": "NodeNext",
        "resolveJsonModule": true,
        "noEmit": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "strict": true,
        "noUncheckedIndexedAccess": true
    }
}
